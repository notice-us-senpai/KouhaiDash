<% if @user.errors.any? %>
  <div class="col s12">
    <div class="alert">
      <p>The form contains <%= pluralize(@user.errors.count, "error") %>.</p>
    </div>
    <ul class='alert'>
    <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
